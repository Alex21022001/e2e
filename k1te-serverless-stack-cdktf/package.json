{
  "name": "k1te-serverless-stack-cdktf",
  "version": "2023.10.1",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "deploy-local": "cdktf deploy local --auto-approve=true",
    "deploy-prod": "cdktf deploy kite --auto-approve=true --var-file=.env",
    "destroy-local": "cdktf destroy local --auto-approve=true",
    "destroy-prod": "cdktf destroy kite --auto-approve=true",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^17.0.3",
    "@cdktf/provider-cloudflare": "^9.0.1",
    "@cdktf/provider-docker": "^9.0.0",
    "cdktf": "^0.18.0",
    "cdktf-cli": "^0.18.0",
    "constructs": "^10.1.211",
    "dotenv": "^16.3.1",
    "iam-floyd": "^0.507.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^20.4.4",
    "jest": "^29.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
